{
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "cSpell.words": ["Tampermonkey", "ehgt", "ehsyringe", "exhentai", "hentai", "omnibox", "typedi"],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.tabSize": 4,
    "files.eol": "\n",
    "json.schemas": [
        {
            "fileMatch": ["manifest.json"],
            "url": "http://json.schemastore.org/chrome-manifest.json"
        }
    ],
    "typescript.tsdk": "node_modules/typescript/lib"
}
